{
  "manifest_version": 3,
  "name": "Gemini Chatbot",
  "description": "Chat with Gemini LLM",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "http://127.0.0.1:7000/"
  ]
}
